{
	"name": "nette/web-project",
	"description": "Nette: Standard Web Project",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"require": {
		"php": "^8.2",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.0",
		"nette/caching": "^3.2",
		"nette/database": "^3.0",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10",
		"lulco/phoenix": "^2.6",
		"tomaj/nette-api": "^2.11",
		"webonyx/graphql-php": "^15.18"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"symfony/thanks": "^1",
		"phpstan/phpstan-nette": "*"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"platform": {
			"php": "8.2.24"
		},
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
